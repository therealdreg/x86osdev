@SUFFIX_LINE@


top_builddir = ..

SHELL = @SHELL@

@SET_MAKE@

CC = @CC@
CXX = @CXX@
CFLAGS = @CFLAGS@ @GUI_CFLAGS@
CXXFLAGS = @CXXFLAGS@ @GUI_CXXFLAGS@

LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
RANLIB = @RANLIB@
LIBTOOL=@LIBTOOL@


.@CPP_SUFFIX@.o:
	$(CXX) @DASH@c $(CXXFLAGS) @CXXFP@$< @OFP@$@


all: make_cmos_image

make_cmos_image@EXE@: make_cmos_image.o
	@LINK_CONSOLE@ make_cmos_image.o

clean:
	@RMCOMMAND@ *.o
	@RMCOMMAND@ make_cmos_image@EXE@

dist-clean: clean
	@RMCOMMAND@ Makefile
